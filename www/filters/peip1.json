    {
      "key":"opt",
      "display":"Option",
      "variable":{
        "Du capteur à la décision":"Du capteur à la décision",
        "IOT":"IOT",
        "LV2":"LV2",
        "Logic Problem Solving":"logic",
        "Matériaux":"Matériaux",
        "Robots":"Robots",
        "Smartphonique":"Smartphonique",
        "Web":"Web"
      },
      "blacklist":["PEIP1 - (Du capteur à la décision|IOT|LV2|Logic Problem Solving|Matériaux|Robots|Smartphonique|Web)"],
      "whitelist":["PEIP1 - {opt}"]
